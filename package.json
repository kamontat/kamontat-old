{
  "name": "kamontat",
  "private": true,
  "description": "personal website for kamontat",
  "version": "0.1.0-alpha.1",
  "author": {
    "name": "Kamontat Chantrachirathumrong",
    "email": "developer@kamontat.net",
    "url": "https://github.com/kamontat"
  },
  "license": "BSD 3-Clause",
  "keywords": [
    "gatsby",
    "kamontat",
    "portfolio",
    "emotion",
    "storybook"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kamontat/kamontat"
  },
  "scripts": {
    "build:website": "gatsby build",
    "build:storybook": "build-storybook --output-dir storybook",
    "build": "yarn build:website && yarn build:storybook",
    "dev": "gatsby develop",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "lint": "eslint . --ignore-path .gitignore --ext .ts,.tsx",
    "test": "jest",
    "test:e2e": "start-test serve http://localhost:9000 cy:open",
    "test:e2e:headless": "start-test serve http://localhost:9000 cy:run",
    "lint:ci": "yarn lint --format json --output-file reports/eslint/ts-lint-result.json && yarn lint --format junit --output-file reports/eslint/ts-lint-result.xml",
    "test:ci": "jest --ci --coverage --runInBand",
    "test:e2e:ci": "start-test serve http://localhost:9000 cy:run:record",
    "aggregate:reports": "nyc",
    "clean": "rimraf node_modules/.cache .cache public reports coverage cypress-coverage .nyc_output junit.xml logs storybook",
    "clean:all": "rimraf node_modules/.cache .cache public reports coverage cypress-coverage .nyc_output junit.xml logs storybook node_modules yarn.lock",
    "storybook": "NODE_ENV=production start-storybook --port 8001",
    "cy:open": "cypress open",
    "cy:open:dev": "cypress open --config baseUrl=http://localhost:8000/",
    "cy:run": "cypress run --headless",
    "cy:run:record": "cypress run --headless --record --key ${CYPRESS_RECORD_KEY:-abc}",
    "import:env": "export $(egrep -v '^#' .env | xargs)"
  },
  "dependencies": {
    "@emotion/core": "10.0.28",
    "@emotion/styled": "10.0.27",
    "emotion-theming": "10.0.27",
    "gatsby": "2.20.15",
    "gatsby-image": "2.3.2",
    "gatsby-plugin-emotion": "4.2.1",
    "gatsby-plugin-gdpr-cookies": "1.0.7",
    "gatsby-plugin-google-tagmanager": "2.2.1",
    "gatsby-plugin-manifest": "2.3.5",
    "gatsby-plugin-netlify": "2.2.1",
    "gatsby-plugin-offline": "3.1.2",
    "gatsby-plugin-react-helmet": "3.2.1",
    "gatsby-plugin-sentry": "1.0.1",
    "gatsby-plugin-sharp": "2.5.4",
    "gatsby-plugin-sitemap": "2.3.1",
    "gatsby-source-contentful": "2.2.7",
    "gatsby-source-filesystem": "2.2.2",
    "gatsby-transformer-sharp": "2.4.4",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-cookie": "4.0.3",
    "react-cookie-consent": "3.0.0",
    "react-dom": "16.13.1",
    "react-helmet": "6.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/preset-typescript": "7.9.0",
    "@cypress/code-coverage": "3.2.2",
    "@storybook/addon-actions": "5.3.18",
    "@storybook/addon-links": "5.3.18",
    "@storybook/addons": "5.3.18",
    "@storybook/react": "5.3.18",
    "@testing-library/cypress": "6.0.0",
    "@types/enzyme": "3.10.5",
    "@types/jest": "25.2.1",
    "@types/node": "12.12.34",
    "@types/react": "16.9.32",
    "@types/react-dom": "16.9.6",
    "@types/react-helmet": "5.0.15",
    "@types/react-test-renderer": "16.9.2",
    "@typescript-eslint/eslint-plugin": "2.26.0",
    "@typescript-eslint/parser": "2.26.0",
    "axe-core": "3.5.3",
    "babel-jest": "25.2.6",
    "babel-loader": "8.1.0",
    "babel-plugin-istanbul": "6.0.0",
    "babel-plugin-remove-graphql-queries": "2.8.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-gatsby": "0.3.1",
    "babel-preset-react-app": "9.1.2",
    "cypress": "4.3.0",
    "cypress-axe": "0.8.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-to-json": "3.4.4",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.19.0",
    "gatsby-plugin-ts-config": "0.2.2",
    "gatsby-plugin-typescript": "2.3.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "25.3.0",
    "jest-chain": "1.1.5",
    "jest-emotion": "10.0.32",
    "jest-extended": "0.11.5",
    "jest-junit": "10.0.0",
    "prettier": "2.0.2",
    "react-test-renderer": "16.13.1",
    "start-server-and-test": "1.10.11",
    "tailwindcss": "1.2.0",
    "twin.macro": "1.0.0-alpha.8",
    "typescript": "3.8.3"
  },
  "nyc": {
    "report-dir": "cypress-coverage"
  }
}
